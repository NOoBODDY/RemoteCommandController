@model WebServer.ViewModels.ModuleViewModel
@{
    ViewData["Title"] = "Добавление нового модуля";
}
<div class="form-group">
    <div class="form-group">
            <label  class="control-label">Название</label>
            <label  class="control-label">@Model.Name</label>
    </div>
    <div class="form-group">
            <label  class="control-label">Тип</label>
            <label  class="control-label">@Model.FileType</label>
    </div>
    <div class="form-group">
            <label  class="control-label"><a asp-action = "Edit" asp-controller = "Modul" asp-route-id = "@Model.Id">Изменить</a></label>
            <label  class="control-label"><a asp-action = "Delete" asp-controller = "Modul" asp-route-id = "@Model.Id">Удалить</a></label>
    </div>
</div>
